  trace("[pano tells parent]",msg);
  if(pano_PARENT_FUNC==null)return; //--auto-tells can be ignored if no parent is registered
  pano_PARENT_FUNC(pano_FRAME_ID,msg,args)
  trace("[msg sent]");
}

