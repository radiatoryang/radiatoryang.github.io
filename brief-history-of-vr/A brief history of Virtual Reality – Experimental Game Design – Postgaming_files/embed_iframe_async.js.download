function init_pano_prefs(prefs){

  //--default vals:

  pano_prefs={

    has_carousel:   true,

    os_wheel_factor:  -1,
    has_map:      true,

    has_map_sidebar:  false,
    map_sidebar_open: false,
    map_style:      null,   //-- the default map_style is in init_pano_map()

    krp_css:    {},

    pano_detail_page: false,

    is_home_main_pano: false,

    portfolio_view: false,

    starts_fullscreen: false,

    pro_page: false

  };

  if(isMac())pano_prefs.os_wheel_factor=1;

  //--apply passed prefs:

  for(curTag in prefs)pano_prefs[curTag]=prefs[curTag];

  //--krp_box_css for size of pano
  if(pano_prefs.is_for_nearpod){
    var krp_box_css={top:0,bottom:60};
  }else{
    var krp_box_css={top:35,bottom:60};
  }

  if(pano_prefs.has_map==false){
    krp_box_css.top=0; //--remove space reserved at top for map-breadcrumbs
  }
  if(pano_prefs.has_carousel==false){
    krp_box_css.bottom=0; //--remove space reserved at bottom for carousel-thumbs
    init_no_carousel();//--additional css (knob-bottom, author info)
  }

  pano_prefs.krp_css=krp_box_css;

  $('#pano_krp').css(pano_prefs.krp_css);

  if(pano_prefs.has_carousel==false){
    hide_pano_thumbs(true)
  }

  // load attempts
  if(pano_prefs.pano_load_max_attempts){
    pano_load_max_attempts=pano_prefs.pano_load_max_attempts;
  }

  if(pano_prefs.pano_load_interval_delay){
    pano_load_interval_delay=pano_prefs.pano_load_interval_delay;
  }
}

