function toggle_gyro () {
  gyro_on = !gyro_on;
  switch_gyro(gyro_on);
}

function switch_gyro(on) {
  var code = "";
  if(on) {
    code += 'set(layer[toggle_gyro_button].url, "/redesign_2014_images/pano/ui/gyro-off.png");';
    code += 'set(plugin[gyro].enabled, true);'
  } else {
    code += 'set(layer[toggle_gyro_button].url, "/redesign_2014_images/pano/ui/gyro-on.png");';
    code += 'set(plugin[gyro].enabled, false);'
  }
  krpano.call(code);
}

