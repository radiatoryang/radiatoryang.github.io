function init_pano360(handle, prefs, data) {
  load_time_debug("start init_pano_360");

  image_handle = handle;

  init_pano_prefs(prefs);

  initPState(prefs);
  init_pano_map(handle, null, null);

  init_reflow_pano();
  //
  init_pano_toys();
  init_fullscreen_toys();
  //
  init_screen_buttons(handle);
  init_projections_menu();
  init_fullscreen_toys();
  //
  init_title_display();
  init_artist_info();
  //
  //--single interval-sweep (polls pano rather than )
  init_update_loop();

  prevent_i_beam_drag('#pano_group');

  init_mouse();

  init_for_devices();

  krpano_HAS_LOADED();

  external_embed_ping(handle);

  load_time_debug("init_pano_360 end");

  if(prefs.is_home_main_pano){
    // Send data performance for home page iframe
    timing_data = prepareTimingPerformance();
    $.getJSON('/data/get_no_of_views', { id: 0, timing_data: timing_data }, function(data) {  });
  }

  $("#krpanoObject").on('click',notify_click);
}

var recorded_interactions = {};

function notify_click() {
  // assuming that the pano was clicked to move it around
  notify_analytics('pano_panned');
}

function notify_analytics(desc,cat) {
  if (cat == undefined) { cat = 'pano_component';}
  if (!recorded_interactions[cat+'_'+desc]) {
    //console.log("notify_analytics: "+desc);
    ga('send', 'event', cat, desc);
    recorded_interactions[cat+"_"+desc] = true;
  }
}

function load_another_pano(handle, data) {
  show_spinner();
  image_handle = handle;
  var xml_url = "/embed_iframe/"+handle+".xml"
  // add the language param to the url to avoid ajax caching after a language change
  xml_url += "?lang="+jQuery.cookie('lang');
  if (pano_prefs.is_home_main_pano) { xml_url += "&home_main_pano=true"; }
  // reset the projection mode to avoid messing up the proj menu after loading the new pano
  krpano.call("enable_normal();");
  krpano.call("loadpano(" + xml_url + ");");
}

function process_pano_metadata(data){
  handle = data.id;
  update_modals(handle, data.title, data.licenseable);
  update_pano_shares(handle, data.title, data.description, data.sharable_preview);
  init_pano_nav(data);
  plot_new_pano(data);
  var mousetype = (localStorage.mousetype == "moveto") ? "qtvr_mode" : "click_drag_mode";
  set_mouse_mode(mousetype);
  setProjection();
}

var is_external_embed = false;

function external_embed_ping(handle){
  if (document.referrer.indexOf("360cities.net") <= -1){
    url = encodeURIComponent(document.referrer.split('/')[2]);
    jQuery.get('https://www.360cities.net/ping/embed/external/'+url+'/'+handle);
    is_external_embed = true;
  }
}

