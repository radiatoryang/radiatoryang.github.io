      if((wndLocStr.indexOf("/image/") > -1) || (window.location.toString().indexOf("force_thumb_click_reload") > -1)){
        // if we are in portfolio mode, stay in portfolio mode
        if( wndLocStr.indexOf("/profile/") > -1){
          wndNewLocStr = wndLocStr.slice(0,wndLocStr.indexOf("/image/")) + "/image/" + id;
        } else if (wndLocStr.indexOf("?set=") > -1) {
          wndNewLocStr = "/image/"+id + wndLocStr.slice(wndLocStr.indexOf("?set="), wndLocStr.length);
        } else if (wndLocStr.indexOf("?curated_set=") > -1) {
          wndNewLocStr = "/image/"+id + wndLocStr.slice(wndLocStr.indexOf("?curated_set="), wndLocStr.length);
        }
        else{
          wndNewLocStr = "/image/"+id;
        }
      }

      if (wndNewLocStr != "")
      {
        if (pano_is_fullscreen || mobile_fullscreen) {
          deferred_loading_url = wndNewLocStr;
        } else {
          top.window.location.href = wndNewLocStr;
          return;
        }
      }
      load_another_pano(id);
      $('#pano_popper').hide();

    }
  catch(err) {
    // external embed will give an error on finding the top window locations
    load_another_pano(id);
    $('#pano_popper').hide();
  }
}

